{
  "name": "ci-cd-example",
  "version": "1.0.0",
  "description": "Full-stack app with AI-powered CI/CD",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start:prod": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start"
  },
  "keywords": ["ci-cd", "github-actions", "ai", "devops"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "cypress": "^13.3.0",
    "concurrently": "^8.2.1"
  },
  "dependencies": {
    "openai": "^4.14.0"
  }
}
